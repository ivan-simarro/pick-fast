{
    "name": "fullstack",
    "author": "ivan_simarro",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "django": "cd ./backend && pip3 install requirements.txt",
        "front": "cd ./frontend && yarn start",
        "back": "cd ./backend && python3 manage.py runserver",
        "app": "npm-run-all --parallel django front back",
        "install": "yarn install",
        "start": "install && app"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5",
        "pure-react-carousel": "^1.28.1"
    }
}