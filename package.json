{
    "name": "fullstack",
    "author": "ivan_simarro",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "env": "cd ./backend && source env/bin/activate",
        "django": "cd ./backend && pip install requirements.txt",
        "front": "cd ./frontend && yarn start",
        "back": "cd ./backend && python3 manage.py runserver",
        "start": "npm-run-all --parallel env django front back"
    },
    "dependencies": {
        "npm-run-all": "^4.1.5",
        "pure-react-carousel": "^1.28.1"
    }
}